<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="chickenmumani.com.allshelf">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.hardware.camera.autofocus" />
    <uses-permission android:name="android.hardware.camera" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="chickenmumani.com.allshelf.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="chickenmumani.com.allshelf.Navi_Activity"
            android:label="@string/title_activity_navi"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="chickenmumani.com.allshelf.SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity
            android:name="chickenmumani.com.allshelf.Export_Activity2"
            android:label="@string/title_activity_export" />
        <activity
            android:name="chickenmumani.com.allshelf.Logout_Activity"
            android:theme="@style/myDialog" />
        <activity
            android:name="chickenmumani.com.allshelf.Message_PopActivity"
            android:theme="@style/myDialog" />
        <activity android:name="chickenmumani.com.allshelf.SignUp_Activity" />
        <activity android:name="chickenmumani.com.allshelf.Notice_ContentActivity" />
        <activity android:name="chickenmumani.com.allshelf.Export_Activity" />
        <activity android:name="chickenmumani.com.allshelf.WithDrawal_Activity" />
        <activity android:name="chickenmumani.com.allshelf.Reviewview_Activity" />
        <activity
            android:name="chickenmumani.com.allshelf.BookInfo_Activity"
            android:theme="@style/myDialog" />
        <activity
            android:name="chickenmumani.com.allshelf.Report_Activity"
            android:theme="@style/myDialog" />
        <activity android:name="chickenmumani.com.allshelf.Follwer_Activity" />
        <activity android:name="chickenmumani.com.allshelf.Search_Activity" />
        <activity android:name="chickenmumani.com.allshelf.Review_WriteActivity"></activity>
    </application>

</manifest>